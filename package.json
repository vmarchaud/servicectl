{
  "name": "servicectl",
  "version": "0.1.1",
  "description": "Servicectl is a opinionated process manager that aim to simplify native init systems for developers.",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@oclif/command": "^1.5.13",
    "@oclif/config": "^1.12.12",
    "@oclif/plugin-autocomplete": "^0.1.2",
    "@oclif/plugin-help": "^2.1.6",
    "@oclif/plugin-not-found": "^1.2.2",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "async": "^3.0.1",
    "await-of": "^1.2.0",
    "cli-ux": "^5.3.0",
    "dbus-next": "^0.5.1",
    "luxon": "^1.16.0",
    "v8-compile-cache": "^2.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^11.13.0",
    "mocha": "^6.1.4",
    "ts-node": "^8.1.0",
    "tslint": "^5.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.3"
  },
  "engines": {
    "node": ">=10.4.0"
  },
  "bin": {
    "servicectl": "./bin/run"
  },
  "oclif": {
    "commands": "./build/cli",
    "bin": "servicectl",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "files": [
    "/bin",
    "/build",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "cli",
    "sysadmin",
    "tools",
    "logs",
    "reload",
    "production",
    "pm2",
    "nodemon",
    "forever",
    "supervisor",
    "process manager",
    "cluster",
    "devops",
    "systemd",
    "launchd",
    "service",
    "systemctl"
  ],
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "copy": "mkdir build/backends/systemd/creators/cluster && cp src/backends/systemd/creators/cluster/node-handle-socket.js build/backends/systemd/creators/cluster/node-handle-socket.js",
    "prepack": "rm -rf build && tsc -b && oclif-dev manifest && oclif-dev readme && yarn copy",
    "test": "mocha -r ts-node/register --forbid-only \"test/**/*.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "tslint --project . src/**/*.ts"
  }
}
